{% extends 'base.html.twig' %}

{% block body %}
    <div class="container">
    {% for prescription in prescriptions %}
{#        {{ dump(prescription) }}#}

        <p>Patient: {{ prescription.userId.id }}</p>
        <div>
            {% for user in users %}
                {{ user.name }}
            {% endfor %}
        </div>
        <p>Voir prescription: <a href="{{ asset('uploads/prescriptions/' ~ prescription.prescriptionFileName) }}">{{ prescription.prescriptionFileName }}</a></p>
        <p><a href="{{ path('app_delete_prescription') }}">Supprimer</a></p>

    <hr>
    {% endfor %}
    </div>
{% endblock %}

{% block title %}
Prescriptions
{% endblock %}

